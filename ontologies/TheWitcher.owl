    </owl:DatatypeProperty>
    


    <!-- https://dbpedia.org/ontology/Person#has_age -->

    <owl:DatatypeProperty rdf:about="https://dbpedia.org/ontology/Person#has_age">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nick/ontologies/2022/3/the-witcher#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- https://dbpedia.org/ontology/Person#is_diffunct -->

    <owl:DatatypeProperty rdf:about="https://dbpedia.org/ontology/Person#is_diffunct">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Witcher_School"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- https://dbpedia.org/ontology/Person#is_intelligent -->

    <owl:DatatypeProperty rdf:about="https://dbpedia.org/ontology/Person#is_intelligent">
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Vampire"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- https://dbpedia.org/ontology/Person#is_weak_to_silver -->

    <owl:DatatypeProperty rdf:about="https://dbpedia.org/ontology/Person#is_weak_to_silver">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Monster"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <owl:propertyDisjointWith rdf:resource="https://dbpedia.org/ontology/Person#is_weak_to_steel"/>
    </owl:DatatypeProperty>
    


    <!-- https://dbpedia.org/ontology/Person#is_weak_to_steel -->

    <owl:DatatypeProperty rdf:about="https://dbpedia.org/ontology/Person#is_weak_to_steel">
