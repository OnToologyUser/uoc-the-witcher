    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://dbpedia.org/ontology/Person#at_peace_with -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#at_peace_with">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Location"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Person#Location"/>
        <owl:propertyDisjointWith rdf:resource="https://dbpedia.org/ontology/Person#is_at_war_with"/>
        <rdfs:comment>A country has a peace treaty with another.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#disables_magic -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#disables_magic">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://dbpedia.org/ontology/Person#magic_disabled_by"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Dimeritium"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Person#Chaos"/>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#has_elder_blood -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#has_elder_blood">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Elder_Blood"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://dbpedia.org/ontology/Person#is_a_sorcerer"/>
                <owl:someValuesFrom rdf:resource="https://dbpedia.org/ontology/Person#Elf"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://dbpedia.org/ontology/Person#is_a_sorcerer"/>
                <owl:someValuesFrom rdf:resource="https://dbpedia.org/ontology/Person#Part-Elf"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment>Who can have Elder Blood?</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#is_a_druid -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#is_a_druid">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Druid"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Person#Human"/>
        <rdfs:comment>Who can be a druid?</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#is_a_sorcerer -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#is_a_sorcerer">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Sorcerer"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://dbpedia.org/ontology/Person#is_a_sorcerer"/>
                <owl:someValuesFrom rdf:resource="https://dbpedia.org/ontology/Person#Elf"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://dbpedia.org/ontology/Person#is_a_sorcerer"/>
                <owl:someValuesFrom rdf:resource="https://dbpedia.org/ontology/Person#Human"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://dbpedia.org/ontology/Person#is_a_sorcerer"/>
                <owl:someValuesFrom rdf:resource="https://dbpedia.org/ontology/Person#Part-Elf"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment>Who can be a sorcerer?</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#is_at_war_with -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#is_at_war_with">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Location"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Person#Location"/>
        <rdfs:comment>A country is at war with another</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#is_weak_to_oil -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#is_weak_to_oil">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Monster"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Person#Oil"/>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#magic_disabled_by -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#magic_disabled_by">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Chaos"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Person#Dimeritium"/>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#use_magic -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#use_magic">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://dbpedia.org/ontology/Person#use_magic"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/nick/ontologies/2022/3/the-witcher#Person"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Person#Chaos"/>
        <rdfs:comment>Who can use magic?</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#use_potion -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#use_potion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Witcher"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Person#Potion"/>
        <rdfs:comment>Who can use potions?</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://dbpedia.org/ontology/Person#use_signs -->

    <owl:ObjectProperty rdf:about="https://dbpedia.org/ontology/Person#use_signs">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/ontology/Person#Witcher"/>
        <rdfs:range rdf:resource="https://dbpedia.org/ontology/Person#Signs"/>
        <rdfs:comment>Who can use signs?</rdfs:comment>
    </owl:ObjectProperty>
    


